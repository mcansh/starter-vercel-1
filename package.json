{
  "name": "remix-starter-vercel",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "postinstall": "tsc -b",
    "predeploy": "remix build && tsc -b",
    "deploy": "vercel",
    "start": "concurrently \"tsc -b -w --preserveWatchOutput\" \"remix run\" \"vercel dev\""
  },
  "dependencies": {
    "@mdx-js/react": "1.6.22",
    "@remix-run/cli": "^0.8.2",
    "@remix-run/data": "^0.8.2",
    "@remix-run/react": "^0.8.2",
    "@remix-run/vercel": "^0.8.2",
    "@vercel/node": "1.8.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12"
  }
}
